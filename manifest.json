{
    "manifest_version": 3,
    "name": "Yahoo Finance Scraper",
    "description": "Scrapes historical data of a valid Yahoo Finance quote.",
    "version": "1.1",
    "permissions": ["activeTab", "scripting", "downloads"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}